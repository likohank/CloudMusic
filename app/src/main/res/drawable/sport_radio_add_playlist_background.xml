<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/sportRadioShareBgColorPrs" />
            <corners android:radius="13.0dip" />
            <solid android:color="@color/translucent" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/sportRadioShareBgColor" />
            <corners android:radius="13.0dip" />
            <solid android:color="@color/translucent" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/sportRadioShareBgColor" />
            <corners android:radius="13.0dip" />
            <solid android:color="@color/translucent" />
        </shape>
    </item>
</selector>