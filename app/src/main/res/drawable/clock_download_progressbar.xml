<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3.0dip" />
            <stroke android:width="1.0dip" android:color="@color/normalC1" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3.0dip" />
                <stroke android:width="1.0dip" android:color="@color/normalC1" />
                <gradient android:startColor="#ffce3d3a" android:endColor="#ffce3d3a" android:angle="270.0" android:centerY="1.0" android:centerColor="#ffce3d3a" />
            </shape>
        </clip>
    </item>
</layer-list>